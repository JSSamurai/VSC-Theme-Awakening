{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "name": "jsdojo-awakening",
    "semanticHighlighting": true,

    "colors": {
        // WTF? Properties that i dont know where in the FooBar they came from!?
        "editorGutter.commentRangeForeground": "#ff00ff", //                  WTF?

