        /*


        ->  JSON: Types ---------------------------------------------> */

        {
            "name": "JSON: Value(string)",
            "scope": "meta.structure.dictionary.value.json",
            "settings": {
                "foreground": "#6da7b8",
                "fontStyle": "italic"
            }
        },

        {
            "name": "JSON: Value(constant)",
            "scope": "constant.language.json",
            "settings": {
                "foreground": "#A44A48",
                "fontStyle": "italic"
