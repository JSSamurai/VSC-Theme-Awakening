        {
            "name": "JAVASCRIPT'Z BASE COLOR",
            "scope": "source.js",
            "settings": {
                "foreground": "#9098A0",
                "fontStyle": ""
            }
        },

        {
            "name": "META/PUNCTUATION ('Braces')",
            "scope": [
                "punctuation.definition.block.js",
                "punctuation.definition.comment.js",
                "meta.brace.square.js",
                "meta.brace.round.js"
            ],
            "settings": {
                "foreground": "#00CCFF90",
                "fontStyle": "bold"
            }
        },

        {
            "name": "PUNCTUATION ('Quotation~Marks')",
            "scope": [
                "punctuation.definition.string.begin.js",
                "punctuation.definition.string.end.js",
                "punctuation.definition.string.template.begin.js",
                "punctuation.definition.string.template.end.js"
            ],
            "settings": {
                "foreground": "#057AA0",
                "fontStyle": "italic bold"
            }
        },

        {
            "name": "PUNCTUATION ('Terminators' & 'Seperators')",
            "scope": [
                "punctuation.terminator.statement.js",
                "punctuation.separator.comma.js",
                "punctuation.separator.key-value.js",
                "punctuation.separator.parameter.js"
            ],
            "settings": {
                "foreground": "#057AA0",
                "fontStyle": ""
            }
        },

        {
            "name": "PUNCTUATION/OPERATOR ('Access-Operator')",
            "scope": ["punctuation.accessor.js"],
            "settings": {
                "foreground": "#0597c7",
                "fontStyle": ""
            }
        },

        {
            "name": "MEMORY STORAGE UNIT(s)",
            "scope": ["storage"],
            "settings": {
                "foreground": "#3088DD",
                "fontStyle": ""
            }
        },

        {
            "name": "MEMORY STORAGE UNIT(s): class, function, phat-arrow/fn-arrow",
            "scope": [
                "storage.type.class.js",
                "storage.type.function.js",
                "storage.type.function.arrow.js"
            ],
            "settings": {
                "foreground": "#207AC8",
                "fontStyle": "bold"
            }
        },

        {
            "name": "MEMORY STORAGE UNIT(s): var, let, const, constructor",
            "scope": "storage.type.js",
            "settings": {
                "foreground": "#207AC8",
                "fontStyle": "bold italic"
            }
        },

        {
            "name": "MEMORY STORAGE UNIT(s): Static, get, set, async",
            "scope": [
                "storage.type.property.js",
                "storage.modifier.async.js",
                "storage.modifier.js"
            ],
            "settings": {
                "foreground": "#3088DD",
                "fontStyle": "italic"
            }
        },

        {
            "name": "OPERATORS: 'Bool Operators, Return Statment & Await Statment')",
