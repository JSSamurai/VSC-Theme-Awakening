        {
            "name": "JSON: L03",
            "scope": "meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json",
            "settings": {
                "foreground": "#CC7030",
                "fontStyle": ""
            }
        },
        /*


 =>   JSON: PUNCTUATION                                                 */
        {
            "name": "JSON: Puncuation (String Quotation-Marks)",
            "scope": [
                "punctuation.support.type.property-name.begin.json",
