    "semanticTokenColors": {
        "class.declaration:javascript": {
            "foreground": "#AAAAAA",
            "bold": true,
            "italic": false,
            "underline": false
        },

        "property:javascript": {
            "foreground": "#AAAAAA",
            "bold": false,
            "italic": false,
            "underline": false
        }
