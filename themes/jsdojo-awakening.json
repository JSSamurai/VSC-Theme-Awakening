    "semanticTokenColors": {
        // "class": {
        //     "foreground": "#00000000",
        //     "bold": false,
        //     "italic": false,
        //     "underline": false
        // },

        "class.declaration:javascript": {
            "foreground": "#B5B5B5",
            "bold": true,
            "italic": false,
            "underline": true
        }
    },

    "tokenColors": [
        /*

  <<=============================/ JAVASCRIPT /=============================>>   */
        {
            "name": "JAVASCRIPT'Z BASE COLOR",
            "scope": "source.js",
            "settings": {
                "foreground": "#9F9F9F",
                "fontStyle": ""
            }
        },

        {
            "name": "PUNCTUATION ('Quotation~Marks')",
            "scope": [
                "punctuation.definition.string.begin.js",
                "punctuation.definition.string.end.js",
                "punctuation.definition.string.template.begin.js",
                "punctuation.definition.string.template.end.js"
            ],
            "settings": {
                "foreground": "#00AAAA",
                "fontStyle": "italic"
            }
        },

        {
            "name": "PUNCTUATION ('Terminators' & 'Seperators')",
            "scope": [
                "punctuation.terminator.statement.js",
                "punctuation.separator.comma.js",
                "punctuation.separator.key-value.js"
            ],
            "settings": {
                "foreground": "#00AAAA",
                "fontStyle": ""
            }
        },

        {
            "name": "PUNCTUATION/OPERATOR ('Access-Operator')",
            "scope": "punctuation.accessor.js",
            "settings": {
                "foreground": "#00AAAA",
                "fontStyle": "bold"
            }
        },

        {
            "name": "META/PUNCTUATION ('Braces')",
            "scope": [
                "punctuation.definition.parameters.begin.js",
                "punctuation.definition.parameters.end.js",
                "punctuation.definition.block.js",
                "punctuation.definition.comment.js",
                "meta.brace.square.js",
                "meta.brace.round.js"
            ],
            "settings": {
                "foreground": "#00AAAA",
                "fontStyle": ""
            }
        },

        {
            "name": "COMMENTS (Line and Block)",
            "scope": ["comment.line.double-slash", "comment.block"],
            "settings": {
                "foreground": "#643854",
                "fontStyle": "italic"
            }
        },

        {
            "name": "COMMENT (Hashtab Comments/.gitignore Comments)",
            "scope": "comment.line.number-sign",
            "settings": {
                "foreground": "#006400",
                "fontStyle": ""
            }
        },

        {
            "name": "COMMENT (Documentation-Block/JSDocs) ",
            "scope": "comment.block.documentation",
            "settings": {
                "foreground": "#005350",
                "fontStyle": "italic"
            }
        },

        {
            "name": "STORAGE CONTAINERS",
            "scope": ["storage.type.class.js", "storage.type.function.js"],
            "settings": {
                "foreground": "#C49624",
                "fontStyle": "bold"
            }
        },

        {
            "name": "CONTAINERS (var/let/const)",
            "scope": "storage.type.js",
            "settings": {
                "foreground": "#C49624",
                "fontStyle": "italic"
            }
        },

        {
            "name": "CONTAINERS (Container-Properties/get/set/static/async)",
            "scope": ["storage.type.property.js", "storage.modifier.async.js"],
            "settings": {
                "foreground": "#AEAA52",
                "fontStyle": "italic"
            }
        },

        {
            "name": "SYMBOLIC OPERATORS",
            "scope": [
                "keyword.operator.assignment.js",
                "keyword.operator.arithmetic.js",
                "keyword.operator.assignment.compound.js",
                "keyword.operator.relational.js",
                "keyword.operator.increment.js"
            ],
            "settings": {
                "foreground": "#D86420",
                "fontStyle": "bold"
            }
        },

        {
            "name": "SYNTACTICAL OPERATORS",
            "scope": [
                "keyword.control.conditional.js",
                "keyword.control.switch.js",
                "keyword.control.trycatch.js",
                "keyword.control.loop.js",
                "keyword.operator.new.js",
                "keyword.control.flow.js"
            ],
            "settings": {
                "foreground": "#d12c8c",
                "fontStyle": "italic"
            }
        },

        {
            "name": "STRINGS (Quotation-Marks)",
            "scope": ["string.quoted.single.js", "string.quoted.double.js"],
            "settings": {
                "foreground": "#006450",
                "fontStyle": "italic"
            }
        },

        {
            "name": "STRINGS (Back-Tics/String-Templates)",
            "scope": ["string.template.js"],
            "settings": {
                "foreground": "#704AAA",
                "fontStyle": "italic"
            }
        },
        /*
         


        "variable.language.this.js",
        "support.type.object.module.js",
        "constant.language.boolean.true.js",
        "constant.language.boolean.false.js",
        "constant.numeric.decimal.js",

        "constant.language.undefined.js",
        "constant.numeric.decimal.js",



 


        =>  JSON: Scope-Levels  ------------------------------------>            */
        {
            "name": "JSON: L01",
            "scope": ["meta.structure.dictionary.json"],
